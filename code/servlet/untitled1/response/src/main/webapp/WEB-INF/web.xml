<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>basicServlet</servlet-name>
    <servlet-class>yulin.servlet.basicServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>basicServlet</servlet-name>
    <url-pattern>/basicServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>b2</servlet-name>
    <servlet-class>yulin.servlet.basicServlet2</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>b2</servlet-name>
    <url-pattern>/b2</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>RequestDispatcher</servlet-name>
    <servlet-class>yulin.servlet.RequestDispatcher</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RequestDispatcher</servlet-name>
    <url-pattern>/RequestDispatcher</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>redirect</servlet-name>
    <servlet-class>yulin.servlet.RedirectServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>redirect</servlet-name>
    <url-pattern>/redirect</url-pattern>
  </servlet-mapping>

</web-app>
